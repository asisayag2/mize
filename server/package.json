{
  "name": "mize-server",
  "version": "1.0.0",
  "type": "module",
  "description": "Mi-ze Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
